{
    "name": "aranyasen/laravel-adminer",
    "description": "Adminer (by Jakub Vrana) wrapper for Laravel 5.5+",
    "keywords": ["adminer", "laravel5", "database", "management", "explorer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aranya Sen",
            "email": "sen.aranya@gmail.com"
        }
